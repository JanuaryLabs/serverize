{
  "name": "serverize",
  "version": "0.60.0",
  "bin": {
    "serverize": "./cli.js"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": "./index.js",
      "import": "./index.js",
      "types": "./src/index.d.ts",
      "default": "./index.js"
    },
    "./docker": {
      "node": "./docker/index.js",
      "import": "./docker/index.js",
      "types": "./docker/packages/docker/src/index.d.ts",
      "default": "./docker/index.js"
    },
    "./dockerfile": {
      "node": "./dockerfile/index.js",
      "import": "./dockerfile/index.js",
      "types": "./dockerfile/packages/dockerfile/src/index.d.ts",
      "default": "./dockerfile/index.js"
    },
    "./dockercompose": {
      "node": "./dockercompose/index.js",
      "import": "./dockercompose/index.js",
      "types": "./dockercompose/packages/dockercompose/src/index.d.ts",
      "default": "./dockercompose/index.js"
    },
    "./utils": {
      "node": "./utils/index.js",
      "import": "./utils/index.js",
      "types": "./utils/packages/utils/src/index.d.ts",
      "default": "./utils/index.js"
    }
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/debug": "^4.1.12",
    "@types/semver": "^7.5.8",
    "@types/ws": "^8.5.13"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@balena/dockerignore": "1.0.2",
    "@inquirer/prompts": "^7.2.1",
    "@january/evaluator": "https://github.com/JanuaryLabs/dist/raw/main/evaluator.tar.gz?v=1.3.0",
    "@january/parser": "https://github.com/JanuaryLabs/dist/raw/main/parser.tar.gz?v=1.3.0",
    "@serverize/client": "0.60.0",
    "boxen": "^8.0.1",
    "chalk": "5.4.1",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^3.2.0",
    "commander": "^12.1.0",
    "debug": "^4.4.0",
    "detect-package-manager": "^3.0.2",
    "dockerfile-ast": "^0.7.0",
    "dotenv": "16.4.7",
    "esbuild": "^0.25.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.2",
    "firebase": "^11.1.0",
    "js-yaml": "^4.1.0",
    "ora": "^8.1.1",
    "parse-duration": "^1.1.1",
    "rxjs": "^7.8.1",
    "semver": "^7.6.3",
    "tus-js-client": "^4.2.3",
    "validator": "13.9.0"
  }
}
